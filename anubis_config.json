{
    "anubis_enable_token": "*",
    "code_start_token": "{",
    "code_end_token": "}",
    "anubisignore": [
        "**venv",
        "__pycache__",
        "Parsing",
        "requirements.txt",
        "anubis_config.json",
        "README.md",
        "cmdline.py",
        "src/"
    ],
    "comment_patterns": {
        ".py":
        {
            "base_comment_pattern": "#",
            "multiline_comment_end": "'''",
            "multiline_comment_start": "'''",
            "language_name": "python",
            "py/object": "__main__.CommentPattern"
        },
        ".c":
        {
            "base_comment_pattern": "//",
            "multiline_comment_end": "*/",
            "multiline_comment_start": "/*",
            "language_name": "c",
            "py/object": "__main__.CommentPattern"
        },
        ".js":
        {
            "base_comment_pattern": "//",
            "multiline_comment_end": "*/",
            "multiline_comment_start": "/*",
            "language_name": "javascript",
            "py/object": "__main__.CommentPattern"
        }
    },
    "anubis_multiline_start_token": "|",
    "anubis_multiline_end_token": "|",
    "py/object": "__main__.Config"
}